{
  "name": "project-name",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css}\"",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts src/",
    "lint-fix": "eslint --fix --ext .jsx,.js,.tsx,.ts src/",
    "test": "react-scripts test",
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "test-coverage": "npm test -- --coverage",
    "view-coverage": "open coverage/index.html",
    "gc": "node templates/copy-templates.js ${0}"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.1",
    "@material-ui/icons": "^4.9.1",
    "@rehooks/component-size": "^1.0.3",
    "fetch-hooks-react": "^0.0.1",
    "polished": "^3.4.1",
    "react": "^16.13.1",
    "react-basic-blocks": "^0.0.3",
    "react-datepicker": "^3.0.0",
    "react-dom": "^16.13.1",
    "react-grid-system": "^7.0.2",
    "react-hook-form": "^5.7.2",
    "react-html-parser": "^2.0.2",
    "react-id-swiper": "^3.0.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-scroll": "^1.7.16",
    "react-spinners": "^0.8.3",
    "source-map-explorer": "^2.4.2",
    "styled-components": "^5.1.0",
    "swiper": "^5.4.2",
    "typescript": "^3.8.3",
    "yup": "^0.29.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/node": "13.13.5",
    "@types/react": "16.9.35",
    "@types/react-datepicker": "^2.11.1",
    "@types/react-dom": "16.9.8",
    "@types/react-html-parser": "^2.0.1",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-scroll": "^1.5.5",
    "@types/styled-components": "^5.1.0",
    "@types/yup": "^0.29.2",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "html"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run format && yarn run lint-fix && git add ."
    }
  }
}
