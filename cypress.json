{
  "screenshotsFolder": "./cypress/snapshots/actual",
  "trashAssetsBeforeRuns": true,
  "env": {
    "failSilently": false
  },
  "chromeWebSecurity": false,
  "reporter": "junit",
  "reporterOptions": {
    "mochaFile": "tests/test-output-[hash].xml",
    "toConsole": true,
    "attachments": true,
    "reportDir": "cypress/reports/tests",
    "overwrite": false,
    "html": false,
    "json": true
  }
}
